    "jquery.animate-number": "0.0.14",
    "jvectormap": "2.0.4",
    "line-awesome": "icons8/line-awesome",
    "magnific-popup": "1.1.0",
    "markdown": "0.5.0",
    "messenger": "git+https://github.com/HubSpot/messenger.git#v1.4.2",
    "moment": "^2.22.2",
    "morris.js": "0.5.0",
    "ng-autosize": "1.1.0",
    "ng2-carouselamos": "3.2.0",
    "ng2-datetime": "1.4.0",
    "ng2-table": "1.3.2",
    "ngx-bootstrap": "^2.0.0-rc.0",
    "popper.js": "1.12.5",
    "rxjs": "^6.3.3",
