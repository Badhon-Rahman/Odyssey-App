{
  "name": "Odyssey",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/router": "^6.1.6",
    "@angular/service-worker": "^6.1.6",
    "@ng-bootstrap/ng-bootstrap": "^3.2.2",
    "@ng-select/ng-select": "^2.13.3",
    "@ngx-loading-bar/router": "1.1.0",
    "@ngx-translate/core": "^10.0.1",
    "angular-draggable-droppable": "^4.0.2",
    "animate.css": "3.5.2",
    "awesome-bootstrap-checkbox": "1.0.0",
    "bootstrap": "^4.1.3",
    "bootstrap-colorpicker": "2.5.1",
    "bootstrap-datepicker": "1.7.0",
    "bootstrap-markdown": "2.10.0",
    "bootstrap-select": "1.13.2",
    "bootstrap-slider": "9.8.0",
    "core-js": "^2.5.6",
    "d3": "^3.5.17",
    "dropzone": "^5.1.1",
    "easy-pie-chart": "^2.1.7",
    "font-awesome": "4.7.0",
    "glyphicons-halflings": "https://github.com/inway/glyphicons-halflings#df545d9e4bdae9b5ffa112fcb70b3f8350929875",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
